<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BorderLinearLayout" >
        <attr name="radius" format="dimension" />
        <attr name="radiusTopLeft" format="dimension" />
        <attr name="radiusTopRight" format="dimension" />
        <attr name="radiusBottomLeft" format="dimension" />
        <attr name="radiusBottomRight" format="dimension" />
        <attr name="borderSize" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="borderPressedColor" format="color" />
        <attr name="touchOpacity" format="float" />
        <attr name="borderStyle" format="enum">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
            <enum name="fill_and_stroke" value="2" />
        </attr>
    </declare-styleable>
</resources>